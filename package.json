{
  "name": "shikimori-player",
  "version": "0.0.0",
  "author": {
    "name": "lijspoop",
    "url": "https://github.com/lijspoop"
  },
  "license": "BSD-2-Clause",
  "userScriptMeta": {
    "name": "Shikimori Player",
    "description": {
      "": "Adds players for watching anime on shikimori.one website",
      "ru": "Добавляет плееры для просмотра аниме на сайте shikimori.one"
    },
    "icon": "https://www.google.com/s2/favicons?sz=64&domain=shikimori.one",
    "match": [
      "*://*.shikimori.one/*",
      "*://*.shikimori.me/*"
    ],
    "grant": [
      "GM_xmlhttpRequest"
    ],
    "run-at": "document-start"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.32",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.11",
    "vite-plugin-monkey": "^4.0.6",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": "^22.11.0",
    "vscode": "^1.22.0"
  },
  "packageManager": "pnpm@9.14.3"
}