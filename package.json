{
  "name": "shikimori-player",
  "version": "1.0.0+beta1",
  "author": {
    "name": "lijspoop",
    "url": "https://github.com/lijspoop"
  },
  "license": "BSD-2-Clause",
  "userScriptMeta": {
    "name": "Shikimori Player",
    "description": {
      "": "Adds players for watching anime on shikimori.one website",
      "ru": "Добавляет плееры для просмотра аниме на сайте shikimori.one"
    },
    "icon": "https://www.google.com/s2/favicons?sz=64&domain=shikimori.one",
    "match": [
      "*://*.shikimori.one/*",
      "*://*.shikimori.me/*"
    ],
    "grant": [
      "GM_xmlhttpRequest"
    ],
    "run-at": "document-start"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "eslint --fix",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@lijspoop/event-handler": "github:lijspoop/event-handler",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "graphql-scalars": "^1.24.0",
    "kodikwrapper": "github:lijspoop/kodikwrapper",
    "pinia": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "vue": "^3.5.13",
    "vue-demi": "^0.14.10"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.10.1",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "sass-embedded": "^1.82.0",
    "type-graphql": "2.0.0-rc.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.11",
    "vite-plugin-graphql-loader": "^4.0.4",
    "vite-plugin-monkey": "^4.0.6",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": "~22.11.0"
  },
  "packageManager": "yarn@1.22.22"
}
